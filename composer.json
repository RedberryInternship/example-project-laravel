{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "aloha/twilio": "^4.0",
        "arku/nova-multiselect": "dev-master",
        "fideloper/proxy": "^4.0",
        "giunashvili/xml-parser": "dev-master",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/support": "^6.8",
        "laravel/framework": "^6.2",
        "laravel/nova": "*",
        "laravel/tinker": "^1.0",
        "laravel/vapor-cli": "^1.4",
        "laravel/vapor-core": "^2.2",
        "pulkitjalan/geoip": "^4.0",
        "rackspace/php-opencloud": "dev-master",
        "redberry/charger-prices": "*",
        "spatie/laravel-schemaless-attributes": "^1.6",
        "spatie/laravel-translatable": "^4.2",
        "spatie/nova-translatable": "^2.0",
        "titasgailius/search-relations": "^1.0",
        "tymon/jwt-auth": "dev-develop"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.3"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/ChargerPrices"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Redberry\\GeorgianCardGateway\\": "packages/redberry/georgian-card-gateway/src"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
